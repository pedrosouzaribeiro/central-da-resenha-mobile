{
  "expo": {
    "name": "Central da Resenha",
    "slug": "central-da-resenha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/BALL.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "src/assets/BALL.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/BALL.png",
        "backgroundColor": "#000000"
      },
      "package": "com.centraldaresenha",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "plugins": [
      "expo-font"
    ],
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content",
      "translucent": false
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "backgroundColor": "#000000",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "093781dd-159d-4859-9544-f1a4250bf40d"
      }
    },
    "owner": "yleo"
